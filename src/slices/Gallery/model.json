{
  "id": "gallery",
  "type": "SharedSlice",
  "name": "Gallery",
  "description": "",
  "variations": [
    {
      "id": "default",
      "name": "Default",
      "docURL": "...",
      "version": "initial",
      "description": "Default",
      "imageUrl": "",
      "primary": {
        "content": {
          "type": "StructuredText",
          "config": {
            "label": "Content",
            "placeholder": "",
            "allowTargetBlank": true,
            "multi": "paragraph,preformatted,heading1,heading2,heading3,heading4,heading5,heading6,strong,em,hyperlink,image,embed,list-item,o-list-item,rtl"
          }
        },
        "background_colour": {
          "type": "Color",
          "config": {
            "label": "Background Colour",
            "placeholder": ""
          }
        },
        "alignment": {
          "type": "Select",
          "config": {
            "label": "Alignment",
            "placeholder": "",
            "options": [
              "Left",
              "Center",
              "Right"
            ],
            "default_value": "Left"
          }
        },
        "columns": {
          "type": "Select",
          "config": {
            "label": "Columns",
            "placeholder": "",
            "options": [
              "1",
              "2",
              "3",
              "4"
            ],
            "default_value": "1"
          }
        },
        "top_padding": {
          "type": "Select",
          "config": {
            "label": "Top Padding",
            "placeholder": "",
            "options": [
              "None",
              "Small",
              "Medium",
              "Large"
            ],
            "default_value": "None"
          }
        },
        "bottom_padding": {
          "type": "Select",
          "config": {
            "label": "Bottom Padding",
            "placeholder": "",
            "options": [
              "None",
              "Small",
              "Medium",
              "Large"
            ],
            "default_value": "None"
          }
        },
        "product_listing": {
          "type": "Boolean",
          "config": {
            "label": "Product listing",
            "placeholder_false": "No",
            "placeholder_true": "Yes",
            "default_value": false
          }
        }
      },
      "items": {
        "image": {
          "type": "Image",
          "config": {
            "label": "Image",
            "constraint": {
              "width": 1000,
              "height": 1000
            },
            "thumbnails": []
          }
        },
        "link": {
          "type": "Link",
          "config": {
            "label": "Link",
            "placeholder": "",
            "allowTargetBlank": true,
            "select": null
          }
        },
        "title": {
          "type": "Text",
          "config": {
            "label": "Title",
            "placeholder": ""
          }
        },
        "caption": {
          "type": "Text",
          "config": {
            "label": "Caption",
            "placeholder": ""
          }
        },
        "product": {
          "type": "Link",
          "config": {
            "label": "Product",
            "select": "document",
            "customtypes": [
              "product"
            ]
          }
        }
      }
    }
  ]
}